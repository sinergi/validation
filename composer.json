{
    "name": "sinergi/validation",
    "type": "library",
    "description": "Simple PHP Validation library",
    "keywords": ["validation"],
    "homepage": "https://github.com/sinergi/validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "Sinergi\\Validation\\": "src/Validation"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinergi\\Validation\\Tests\\": ["tests/Validation/Tests", "tests/Validation/Tests/_includes"]
        }
    }
}